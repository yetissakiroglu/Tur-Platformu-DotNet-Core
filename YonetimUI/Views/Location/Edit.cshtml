    <script src="/npanel/vendor/typeahead/handlebars.min.js" type="text/javascript"></script>
    <script src="/npanel/vendor/typeahead/typeahead.bundle.min.js" type="text/javascript"></script>
    <!-- END PAGE LEVEL PLUGINS -->
